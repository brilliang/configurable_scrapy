{
  "name": "ent.qq.com",
  "media_type": "news",
  "links": [
    {
      "type": "const_list",
      "value": [
        "http://ent.qq.com/star/",
        "http://ent.qq.com/"
      ],
      "links": [
        {
          "type": "const",
          "value": "http://ent.qq.com/c/mxzx_1.htm",
          "page_link": {
            "type": "reg_url",
            "value": [
              "@url",
              "mxzx\\_(\\d+)\\.",
              "http://ent.qq.com/c/mxzx_$({1}+1).htm",
              {"length": 100}
            ]
          },
          "links": [
            {
              "type": "link_extractor",
              "doc": "main",
              "value": {"allow": "http://ent.qq.com/a/\\d+/\\d+.htm"},
              "items": {
                "type": {
                  "type": "const",
                  "value": "main"
                },
                "title": {
                  "type": "xpath",
                  "value": "//title/text()"
                },
                "content": {
                  "type": "xpath",
                  "value": "//div[@id='Cnt-Main-Article-QQ']"
                },
                "crumb": {
                  "type": "xpath",
                  "value": "//div[@class='blkBreadcrumbLink']"
                },
                "subtitle": {
                  "type": "const",
                  "value": ""
                },
                "abstraction": {
                  "type": "xpath",
                  "value": "//p[@class='titdd-Article']/text()"
                },
                "description": {
                  "type": "xpath",
                  "value": "//meta[@name='description']/@content"
                },
                "keywords": {
                  "type": "xpath",
                  "value": "//meta[@name='keywords']/@content"
                },
                "source_name": {
                  "type": "xpath",
                  "value": "//span[@class='color-a-0']/text()"
                },
                "source_url": {
                  "type": "xpath",
                  "value": "//span[@class='color-a-0']/a/@href"
                },
                "pub_time": {
                  "type": "xpath",
                  "value": "//span[@class='article-time']/text()",
                  "format": "timestamp"
                },
                "other_list": {
                  "type": "xpath",
                  "value": "//div[@class='bestSelect']"
                },
                "image_urls": {
                  "type": "xpath",
                  "list": true,
                  "value": "//div[@id='Cnt-Main-Article-QQ']//img/@src"
                },
                "articleid": {
                  "type": "xpath_reg",
                  "value": [
                    "//body",
                    "\\cmt_id\\s*=\\s*(\\d+)"
                  ]
                }
              },
              "links": [
                {
                  "type": "meta_replace",
                  "doc": "comment",
                  "value": "http://coral.qq.com/article/{articleid}/comment?commentid=0&reqnum=50&tag=&callback=mainComment",
                  "items": {
                    "id": {
                      "type": "const",
                      "value": "0"
                    },
                    "play_times": {
                      "type": "json",
                      "value": ""
                    }
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}

